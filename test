                    v.HazeESP.MaxDistance = "inf"
                end
            end
            for i,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
                if v:FindFirstChild("HazeESP") then
                    v.HazeESP.Size = UDim2.new(50,50,50,50)
                    v.HazeESP.MaxDistance = "inf"
                end
            end
        end
    end)
end
