                                end
                            end
                        end
                    else
                        topos(CFrame.new(-9570.033203125, 315.9346923828125, 6726.89306640625))
                    end
                else
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Bones","Buy",1,1)
                end
            end
        end)
    end
end
