spawn(function()
while wait() do
    if _G.AutoAdvanceDungeon then
        pcall(function()
            if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Bird-Bird: Phoenix") or game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Bird-Bird: Phoenix") then
                if game.Players.LocalPlayer.Backpack:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
                    if game.Players.LocalPlayer.Backpack:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Level.Value >= 400 then
                        topos(CFrame.new(-2812.76708984375, 254.803466796875, -12595.560546875))
                        if (CFrame.new(-2812.76708984375, 254.803466796875, -12595.560546875).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                            wait(1.5)
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SickScientist","Check")
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SickScientist","Heal")
                        end
                    end
                elseif game.Players.LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
                    if game.Players.LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Level.Value >= 400 then
                        topos(CFrame.new(-2812.76708984375, 254.803466796875, -12595.560546875))
                        if (CFrame.new(-2812.76708984375, 254.803466796875, -12595.560546875).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                            wait(1.5)
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SickScientist","Check")
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SickScientist","Heal")
                        end
                    end
                end
            end
        end)
    end
end
end)
local Saber = Weapon:Label("No : Saber")
local Rengoku = Weapon:Label("No : Rengoku")
local Midnight_Blade = Weapon:Label("No : Midnight Blade")
local Dragon_Trident = Weapon:Label("No : Dragon Trident")
local Yama = Weapon:Label("No : Yama")
local Buddy_Sword = Weapon:Label("No : Buddy Sword")
local Canvander = Weapon:Label("No : Canvander")
local Twin_Hooks = Weapon:Label("No : Twin Hooks")
local Spikey_Trident = Weapon:Label("No : Spikey Trident")
local Hallow_Scythe = Weapon:Label("No : Hallow Scythe")
local Dark_Dagger = Weapon:Label("No : Dark Dagger")
local Tushita = Weapon:Label("No : Tushita")
spawn(function()
while task.wait() do
pcall(function()
    for i,v in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("getInventoryWeapons")) do
        if v.Name == "Saber" then
            Saber:Set("Yes : Saber")
        end
        if v.Name == "Rengoku" then
            Rengoku:Set("Yes : Rengoku")
        end
        if v.Name == "Midnight Blade" then
            Midnight_Blade:Set("Yes : Midnight Blade")
        end
        if v.Name == "Dragon Trident" then
            Dragon_Trident:Set("Yes : Dragon Trident")
        end
        if v.Name == "Yama" then
            Yama:Set("Yes : Yama")
        end
        if v.Name == "Buddy Sword" then
            Buddy_Sword:Set("Yes : Buddy Sword")
        end
        if v.Name == "Canvander" then
            Canvander:Set("Yes : Canvander")
        end
        if v.Name == "Twin Hooks" then
            Twin_Hooks:Set("Yes : Twin Hooks")
        end
        if v.Name == "Spikey Trident" then
            Spikey_Trident:Set("Yes : Spikey Trident")
        end
        if v.Name == "Hallow Scythe" then
            Hallow_Scythe:Set("Yes : Hallow Scythe")
        end
        if v.Name == "Dark Dagger" then
            Dark_Dagger:Set("Yes : Dark Dagger")
        end
        if v.Name == "Tushita" then
            Tushita:Set("Yes : Tushita")
         end
    end
end)
end
end)


local plyserv = Combat:Label("Players")

spawn(function()
while wait() do
    pcall(function()
        for i,v in pairs(game:GetService("Players"):GetPlayers()) do
            if i == 12 then
                plyserv:Set("Players :".." "..i.." ".."/".." ".."12".." ".."(Max)")
            elseif i == 1 then
                plyserv:Set("Player :".." "..i.." ".."/".." ".."12")
            else
                plyserv:Set("Players :".." "..i.." ".."/".." ".."12")
            end
        end
    end)
end
end)

Playerslist = {}

for i,v in pairs(game:GetService("Players"):GetChildren()) do
table.insert(Playerslist,v.Name)
end

