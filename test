    _G.TeleportNPC = value
    if _G.TeleportNPC == true then
        repeat wait()
            if _G.SelectNPC == "Dargon Berath" then
                topos(CFrame.new(703.372986, 186.985519, 654.522034, 1, 0, 0, 0, 1, 0, 0, 0, 1))
            elseif _G.SelectNPC == "Mtsterious Man" then
