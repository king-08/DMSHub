function Update:Nof(txt,tine)
spawn(function()
    local Nof1 = Instance.new("Frame")
    local Nof2 = Instance.new("Frame")
    local Nof3 = Instance.new("Frame")
    local NofLabel = Instance.new("TextLabel")
